    <UserControl.Resources>
        <Style x:Key="HmiGroupBox" TargetType="{x:Type GroupBox}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type GroupBox}">
                        <Grid>
                            <!-- Grid Rows split the GroupBox into two areas -->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Header area-->
                            <Border
                                Grid.Row="0"
                                Background="#481BD68D"
                                BorderBrush="#FF775318"
                                BorderThickness="1"
                                CornerRadius="2,2,0,0" >
                                <ContentPresenter
                                    Margin="4"
                                    ContentSource="Header"
                                    RecognizesAccessKey="True" />
                            </Border>
                            <!-- Main client area -->
                            <Border
                                Grid.Row="1"
                                Background="#DCDFE6E2"
                                BorderBrush="#FF775318"
                                BorderThickness="1,0,1,3"
                                CornerRadius="0,0,2,2" >
                                <ContentPresenter
                                    Margin="4" />
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="ToolTip">
            <Setter Property = "HorizontalOffset" Value="10"/>
            <Setter Property = "VerticalOffset" Value="10"/>
            <Setter Property = "Background" Value="Black"/>
            <Setter Property = "Foreground" Value="AliceBlue"/>
            <Setter Property = "FontSize" Value="14"/>
            <Setter Property = "FontWeight" Value="Bold"/>
        </Style>
        <Style TargetType="{x:Type TextBlock}">
            <Setter Property="Control.FontFamily" Value=" "/>
            <Setter Property="Control.FontSize" Value="12"/>
            <Setter Property="Background">
                <Setter.Value>
                    <SolidColorBrush Color="AliceBlue" Opacity="0.6"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Control.Padding" Value="5"/>

        </Style>
        <Style TargetType="{x:Type TextBox}">
            <Style.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="20" />
                    <Setter Property="BorderThickness" Value="1" />
                </Style>
            </Style.Resources>
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="FontFamily" Value="calibri"/>
            <Setter Property="Foreground" Value="AliceBlue"/>
            <Setter Property="Background">
                <Setter.Value>
                    <SolidColorBrush Color="Black" Opacity="0.6"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Padding" Value="6"/>
            <Setter Property="IsReadOnly" Value="True"/>
            <Setter Property="Focusable" Value="false"/>

            <Setter Property="Cursor" Value="Arrow"/>
        </Style>
        <Style TargetType="{x:Type WPF:Valve}">
            <Setter Property="ToolTip" Value="Зеленый=  сигнал на открытие &#xA; Если Красный= сигнал на закрытие  &#xA; Если Оранжевый= модуль управления не отвечает, обратитесь за диагностикой В КИПиА  "/>
        </Style>
        <Style TargetType="{x:Type WPF:Label}">
            <Setter Property="ToolTip" Value="Зеленый=сигнал c обратной связью &#xA; Серый= текущая уставка  "/>
        </Style>
        <DropShadowEffect x:Key="dropShadow"  ShadowDepth="3" Opacity="0.2"/>
        <Style TargetType="{x:Type Border}">
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Effect" Value="{StaticResource dropShadow}" />
            <Setter Property="CornerRadius" Value="8,8,3,3" />
            <Setter Property="BorderBrush" Value="DarkGray" />
            <Setter Property="BorderThickness" Value="1" />

        </Style>
        <converters1:SingleSeondsToStringTime x:Key="SingleSeondsToStringTime" />
        <converters1:PowerPercentConverter x:Key="PowerPercentConverter" />
        <convertors:BooleanNegateConverter x:Key="BooleanNegateConverter" />



