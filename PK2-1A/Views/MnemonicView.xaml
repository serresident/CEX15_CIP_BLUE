    <UserControl.Resources>
        <Style x:Key="HmiGroupBox" TargetType="{x:Type GroupBox}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type GroupBox}">
                        <Grid>
                            <!-- Grid Rows split the GroupBox into two areas -->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Header area-->
                            <Border
                                Grid.Row="0"
                                Background="#481BD68D"
                                BorderBrush="#FF775318"
                                BorderThickness="1"
                                CornerRadius="2,2,0,0" >
                                <ContentPresenter
                                    Margin="4"
                                    ContentSource="Header"
                                    RecognizesAccessKey="True" />
                            </Border>
                            <!-- Main client area -->
                            <Border
                                Grid.Row="1"
                                Background="#DCDFE6E2"
                                BorderBrush="#FF775318"
                                BorderThickness="1,0,1,3"
                                CornerRadius="0,0,2,2" >
                                <ContentPresenter
                                    Margin="4" />
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="TextBlock">
            <Setter Property="Control.FontFamily" Value=" "></Setter>
            <Setter Property="Control.FontSize" Value="12"></Setter>

            <Setter Property="Control.Padding" Value="5"></Setter>

        </Style>

        <Style TargetType="WPF:Valve">
            <Setter Property="Control.ToolTip" Value="Зеленый=  сигнал на открытие \n Красный= сигнал на закрытие  "></Setter>
        </Style>
        <Style TargetType="WPF:Label">
            <Setter Property="Control.ToolTip" Value="Зеленый=сигнал c обратной связью \n Серый= текущая уставка  "></Setter>
        </Style>
        <Style TargetType="WPF:Motor">
            <Setter Property="Control.Foreground" Value="Black"></Setter>


        </Style>


        <converters1:PowerPercentConverter x:Key="PowerPercentConverter" />
    </UserControl.Resources>

